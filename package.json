{
  "name": "rds-iam-auth",
  "version": "1.0.0",
  "description": "RDS IAM Authentication Example",
  "scripts": {
    "clean": "rimraf cdk.out coverage",
    "deploy": "npm run clean && cdk deploy --require-approval never --outputs-file config.json",
    "destroy": "cdk destroy --force",
    "diff": "cdk diff",
    "lint": "eslint . --ext=.js,.ts",
    "synth": "cdk synth --no-staging > template.yaml",
    "test": "jest --coverage --silent"
  },
  "keywords": [
    "AWS",
    "CDK",
    "IAM",
    "RDS",
    "Authentication",
    "MySQL",
    "PostgreSQL"
  ],
  "author": "Matt Morgan",
  "license": "ISC",
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.0.0-alpha.11",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.0.0-alpha.11",
    "aws-cdk-lib": "^2.0.0",
    "aws-sdk": "^2.1036.0",
    "constructs": "^10.0.9",
    "mysql2": "^2.3.3",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.73",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.14.37",
    "@types/pg": "^8.6.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "esbuild": "^0.13.15",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
